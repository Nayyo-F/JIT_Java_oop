class Animal {
  String name;
  public void play() {
    System.out.println("I can play");
  }
}

class Dog extends Animal {
  public void display() {
    System.out.println("My name is " + name);
  }
}

class Main {
  public static void main(String[] args) {
    Dog labrador = new Dog();
    labrador.name = "buchi";
    labrador.display();
    labrador.eat();

  }
}

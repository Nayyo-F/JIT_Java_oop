
abstract class Human {

  public abstract void cry();   //abstract method

  public void sleep() {
    System.out.println("Zzz...");
  }
}

class kebede extends Human {
  public void cry() {
    System.out.println("kebede crys!");
  }
}

class Main {
  public static void main(String[] args) {
    kebede emotion = new kebede();
    emotion.cry();
    emotion.sleep();
  }
}
